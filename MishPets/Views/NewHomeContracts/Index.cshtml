@model IEnumerable<MishPets.Models.NewHomeContract>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Договоры "Новый дом"</h2>

<p>
    @Html.ActionLink("Добавить", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pet.NickName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateHomeStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdUserForNewHome)
        </th>
        <td>
            @Html.DisplayNameFor(model => model.ApplicationUser.Email)
        </td>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pet.NickName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateHomeStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdUserForNewHome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApplicationUser.Email)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.NewHomeContractId }) |
            @Html.ActionLink("Подробнее", "Details", new { id=item.NewHomeContractId }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.NewHomeContractId })
        </td>
    </tr>
}

</table>
